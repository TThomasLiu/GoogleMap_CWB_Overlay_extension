{
	"manifest_version": 3,
	"name": "Hello Extensions",
	"description": "Base Level Extension",
	"version": "1.0",
	"action": {
		"default_popup": "hello.html",
		"default_icon": "hello.png"
	},
	"content_scripts": [
		{
			"matches": ["https://www.google.com.tw/maps/*"],
			"js": ["inject/inject.js"]
		}
	]
}